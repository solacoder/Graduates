@model Graduates.ViewModel.Dtos.SetupValueParam

<script src="~/js/bootstrap-confirmation.min.js"></script>

<!-- begin panel -->
<div class="panel panel-inverse">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
        </div>
        <div class="panel-title">
            <div class="label m-r-10 panel-heading-btn pull-left">
                <a asp-action="Index" asp-controller="SetupName" class="btn btn-xs btn-icon btn-circle" id="back-btn" data-toggle="tooltip" title="Back To Setup Name" data-placement="right"> <i class="fa fa-arrow-left"></i> </a>
                <a asp-action="create" data-setupname-id="@Model.SetUpNameId" class="btn btn-xs btn-icon btn-circle" data-parent-id="@Model.ParentId" id="load-form" title="@Model.Description"> <i class="fa fa-plus fa-lg"></i> </a>
            </div>
            Setup Value
        </div>
    </div>
    <div class="panel-body">
        <table id="data-table" class="table table-striped table-bordered table-hover"></table>
    </div>

    <!-- #modal-dialog -->
    <div class="modal fade" id="modal-dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Setup Value</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                </div>
                <div id="modal-body" class="modal-body">

                </div>
                <div class="modal-footer">
                    <a href="javascript:;" class="btn btn-white" data-dismiss="modal">Close</a>
                    <button id="btnSubmit" onclick="formSubmit()" class="btn btn-success">Submit</button>
                </div>
            </div>
        </div>
    </div>

</div>
<!-- end panel -->

<script>
    $(function () {
        $('[data-toggle=confirmation]').confirmation({
            rootSelector: '[data-toggle=confirmation]'
        });
    })
</script>

<script type="text/javascript" src="~/js/app/setup-value/setupvalue.js"></script>


